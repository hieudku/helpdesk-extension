{
  "name": "HelpdeskAI",
  "description": "AI Helpdesk integrated with OpenWebUI backend",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage",  "activeTab", "scripting"],
  "host_permissions": [
    "http://10.200.108.66:8080/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
